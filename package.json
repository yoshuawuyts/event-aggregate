{
  "name": "event-aggregate",
  "version": "1.0.3",
  "description": "Funnels events through a single point, so not all of your code needs to attach listeners to every object.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha --recursive -R spec",
    "coverage": "node --harmony-generators ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report text --recursive -- -R spec && rm -rf ./coverage/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yoshuawuyts/event-aggregate"
  },
  "keywords": [
    "events",
    "funnel",
    "aggregate",
    "listener"
  ],
  "author": "Yoshua Wuyts <i@yoshuawuyts.com>",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^2.10.0",
    "istanbul": "^0.2.11",
    "mocha": "^1.21.4",
    "should": "^4.0.4"
  },
  "dependencies": {
    "debug": "^2.6.7"
  }
}
